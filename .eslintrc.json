{
  "extends": ["next", "prettier", "plugin:import/recommended"],
  "rules": {
    "no-console": ["error"],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ],
    "react-hooks/exhaustive-deps": "off"
  }
}
